        custom-checkbox {
  display: flex;
  align-items: center;
  cursor: pointer;
  user-select: none;
}

.custom-checkbox input {
  display: none; /* Hide the default checkbox */
}

.checkbox-icon {
  width: 24px;
  height: 24px;
  border: 2px solid #000; /* Primary color */
  border-radius: 4px;
  margin-right: 8px;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: background-color 0.2s, border-color 0.2s;
}

.custom-checkbox input:checked + .checkbox-icon {
  background-color: #000; /* Primary color when checked */
  border-color: #000; /* Change border color */
}

.checkbox-icon::after {
  content: '';
  width: 10px;
  height: 10px;
  background-color: white; /* Inner square */
  border-radius: 2px;
  opacity: 0;
  transition: opacity 0.2s;
}

.custom-checkbox input:checked + .checkbox-icon::after {
  opacity: 1; /* Show inner square when checked */
}
